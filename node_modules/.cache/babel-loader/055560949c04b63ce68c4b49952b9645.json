y-color\">\n        <td v-if=\"list.shipping.props.in_stock\">ready to ship <v-icon>home</v-icon></td>\n         <td v-else>ready to ship <v-icon>nice</v-icon></td>\n        <td>  in stock </td>\n        <td>  fast dispach </td>\n     \n    </tr>\n    <p>{{list.name}} {{list.tags[0]}}</p>\n     <p>{{list.reviews.rating}} {{list.reviews.count}}reviews {{list.reviews.total_buyers}} buyers</p>\n     <hr>\n   \n        <p>{{list.options.battery_accessories.price.currency.symbol}}{{list.options.battery_accessories.price.value}} - {{list.options[\"4k\"].price.currency.symbol}}{{list.options[\"4k\"].price.value}} /options 2 options (Min order)</p>\n         <p>{{list.options.battery_accessories.price.currency.symbol}}{{list.options.battery_accessories.old_price.value}} - {{list.options[\"4k\"].price.currency.symbol}}{{list.options[\"4k\"].old_price.value}}</p>\n         <hr>\n         <tr >\n        <td></td>\n        <td>  Freee Shipping(Up to $40)    </td>\n        <td>  On time delivery gauranteed </td>\n      </tr>\n   <p>{{list.discount.amount}} OFF ends in {{list.discount.end_date}}</p>\n\n      \n        <p>Options: {{list.options[\"1080p\"].label}}</p>\n        <p> {{list.options[\"4k\"].label}}</p>\n        <p>  {{list.options.battery_accessories.label}}</p>\n        \n     \n</v-card>\n\n    </div>\n    \n</template>\n<script>\nimport Vue from 'vue';\nimport axios from 'axios'\nimport VueAxios from 'vue-axios'\n\nVue.use(VueAxios,axios)\nexport default {\n    name: \"AssessmentData\",\n    data(){\n return {list:undefined}\n    },\n\n   mounted(){\n       \n      Vue.axios.get('https://fe-assignment.vaimo.net/')\n       .then((resp)=>{\n           this.list=resp.data.product\n           console.warn(resp.data.product)\n       }\n       )\n   }\n}\n</script>\n<style lang=\"sass\">\n    @import 'assets/variable.scss';\n    .my-color {\n        color: $primary-color;\n    }\n"]},"metadata":{},"sourceType":"module"}